{
  "name": "framework-nextjs",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ],
    "*.ts": [
      "prettier --list-different",
      "eslint"
    ],
    "*.md": "prettier --list-different"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.18",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss-nested": "^6.0.1",
    "prettier": "3.2.5",
    "typescript": "^5"
  }
}
